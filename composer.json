{
    "name": "firebird/image-trait",
    "description": "Adds a trait for an image attached to Eloquent models",
    "homepage": "https://github.com/fire-birdtech/image-trait",
    "require": {
        "php": "^8.2",
        "illuminate/http": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FireBird\\HasImage\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jarom Rial",
            "email": "jarom@firebirdtech.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    }
}
